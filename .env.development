# Development Environment Configuration for SEO Bot
# Copy this to .env and customize for your local development

#################################
# Application Configuration
#################################
NODE_ENV=development
ENVIRONMENT=development
APP_NAME="SEO Bot (Dev)"
APP_URL=http://localhost:8080
DEBUG=true
LOG_LEVEL=debug

#################################
# Database Configuration
#################################
# Local PostgreSQL Database URL
DATABASE_URL=postgresql://postgres:password@localhost:5432/seo_bot_dev

# Connection pool settings
DB_POOL_SIZE=5
DB_MAX_OVERFLOW=0
DB_POOL_TIMEOUT=30

#################################
# Redis Configuration
#################################
REDIS_URL=redis://localhost:6379/0
# REDIS_PASSWORD=  # No password for local development

#################################
# Frontend Configuration
#################################
VITE_SUPABASE_URL=http://localhost:54321
VITE_SUPABASE_ANON_KEY=your_local_supabase_anon_key
VITE_APP_URL=http://localhost:8080
VITE_APP_NAME="SEO Bot (Dev)"
VITE_ENABLE_ANALYTICS=false
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_test_key

#################################
# Backend API Configuration
#################################
SECRET_KEY=dev_secret_key_for_local_development_only
JWT_SECRET_KEY=dev_jwt_secret_key_for_local_only
JWT_ACCESS_TOKEN_EXPIRES=24
API_V1_STR=/api/v1

#################################
# External API Keys (Development/Test)
#################################
# Google APIs (Optional for development)
# GOOGLE_SEARCH_CONSOLE_CREDENTIALS_FILE=config/gsc-dev-credentials.json
# PAGESPEED_API_KEY=your_dev_pagespeed_api_key
# GOOGLE_API_KEY=your_dev_google_api_key

# SEO APIs (Optional for development)
# SERP_API_KEY=your_dev_serpapi_key
# DATAFORSEO_LOGIN=your_dev_dataforseo_login
# DATAFORSEO_PASSWORD=your_dev_dataforseo_password

# AI Services (Optional for development)
# OPENAI_API_KEY=sk-your_dev_openai_api_key
# ANTHROPIC_API_KEY=your_dev_anthropic_api_key

# Payment Processing (Test Mode)
# STRIPE_SECRET_KEY=sk_test_your_stripe_test_secret_key
# STRIPE_WEBHOOK_SECRET=whsec_your_test_webhook_secret

#################################
# Email Configuration (Development)
#################################
# Use console backend for development
SMTP_HOST=localhost
SMTP_PORT=1025
SMTP_USER=
SMTP_PASSWORD=
SMTP_TLS=false
FROM_EMAIL=dev@localhost

#################################
# Monitoring & Observability
#################################
# SENTRY_DSN=  # Disabled for development
PROMETHEUS_ENABLED=false
# GRAFANA_ADMIN_PASSWORD=dev_password

# OpenTelemetry (Optional)
# OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:14268/api/traces
OTEL_SERVICE_NAME=seo-bot-dev
OTEL_RESOURCE_ATTRIBUTES=service.version=dev,environment=development

#################################
# File Storage (Local)
#################################
# Local file storage for development
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_DEFAULT_REGION=us-east-1
# S3_BUCKET_NAME=
# BACKUP_S3_BUCKET=

#################################
# Security Configuration (Relaxed for Dev)
#################################
ALLOWED_HOSTS=localhost,127.0.0.1,0.0.0.0
CORS_ORIGINS=http://localhost:8080,http://127.0.0.1:8080,http://localhost:3000
SECURE_SSL_REDIRECT=false
SECURE_COOKIES=false
SESSION_COOKIE_SECURE=false
CSRF_COOKIE_SECURE=false

#################################
# Rate Limiting (Very Permissive)
#################################
RATE_LIMIT_PER_MINUTE=1000
RATE_LIMIT_BURST=2000
AUTH_RATE_LIMIT_PER_MINUTE=100

#################################
# Background Jobs
#################################
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2
CELERY_WORKER_CONCURRENCY=1
CELERY_TASK_TIME_LIMIT=600

#################################
# Performance & Caching
#################################
CACHE_TTL=300  # 5 minutes for development
# STATIC_FILES_CDN=  # No CDN for development
ENABLE_GZIP=false
ENABLE_BROTLI=false

#################################
# Feature Flags
#################################
ENABLE_REGISTRATION=true
ENABLE_AI_CONTENT=false  # Disabled by default to save API costs
ENABLE_WEBHOOKS=false   # Disabled for local dev
ENABLE_REALTIME_UPDATES=true

#################################
# Backup Configuration
#################################
BACKUP_RETENTION_DAYS=3
# BACKUP_SCHEDULE=  # Manual backups only in development
# BACKUP_ENCRYPTION_KEY=  # Not needed for dev