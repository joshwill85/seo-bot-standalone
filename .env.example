# Environment Configuration Template for SEO Bot
# Copy this file to .env and customize for your environment
# For specific environments, use .env.development, .env.staging, or .env.production

#################################
# Application Configuration
#################################
NODE_ENV=development
ENVIRONMENT=development
APP_NAME="SEO Bot"
APP_URL=http://localhost:8080
DEBUG=true
LOG_LEVEL=debug

#################################
# Database Configuration
#################################
DATABASE_URL=postgresql://postgres:password@localhost:5432/seo_bot_dev

#################################
# Frontend Configuration
#################################
VITE_SUPABASE_URL=http://localhost:54321
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
VITE_APP_URL=http://localhost:8080
VITE_APP_NAME="SEO Bot"
VITE_ENABLE_ANALYTICS=false
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key

#################################
# Backend API Configuration
#################################
SECRET_KEY=your_secret_key_min_32_characters_long
JWT_SECRET_KEY=your_jwt_secret_key_min_32_characters
JWT_ACCESS_TOKEN_EXPIRES=24
API_V1_STR=/api/v1

#################################
# External API Keys (Optional)
#################################
# GOOGLE_SEARCH_CONSOLE_CREDENTIALS_FILE=path/to/gsc-credentials.json
# PAGESPEED_API_KEY=your_pagespeed_insights_api_key
# OPENAI_API_KEY=sk-your-openai-api-key
# SERP_API_KEY=your_serpapi_key
# DATAFORSEO_LOGIN=your_dataforseo_login
# DATAFORSEO_PASSWORD=your_dataforseo_password

#################################
# Redis Configuration
#################################
REDIS_URL=redis://localhost:6379/0

#################################
# Security Configuration
#################################
ALLOWED_HOSTS=localhost,127.0.0.1
CORS_ORIGINS=http://localhost:8080,http://127.0.0.1:8080
SECURE_SSL_REDIRECT=false
SECURE_COOKIES=false

#################################
# Feature Flags
#################################
ENABLE_REGISTRATION=true
ENABLE_AI_CONTENT=false
ENABLE_WEBHOOKS=false
ENABLE_REALTIME_UPDATES=true

#################################
# Email Configuration (Development)
#################################
SMTP_HOST=localhost
SMTP_PORT=1025
FROM_EMAIL=dev@localhost

# For production, uncomment and configure:
# SMTP_USER=your_smtp_username
# SMTP_PASSWORD=your_smtp_password
# SMTP_TLS=true